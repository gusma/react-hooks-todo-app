{
  "name": "react-todo-app-hooks-and-context",
  "private": true,
  "version": "1.0.0",
  "description": "A highly testable TodoList app that uses React hooks and Context.",
  "keywords": [],
  "main": "src/index.js",
  "dependencies": {
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "react": "next",
    "react-dom": "next",
    "react-scripts": "2.0.3"
  },
  "devDependencies": {
    "jest-junit": "^10.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest",
    "coverage": "jest --coverage --coverageReporters=cobertura",
    "eject": "react-scripts eject"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
